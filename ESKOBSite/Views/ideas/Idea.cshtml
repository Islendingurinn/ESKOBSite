@model ESKOBSite.Models.Idea
@{
    ViewBag.Title = "Idea";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Idea</h2>
<p>@Model.Id</p>
<p>@Model.Title</p>
<p>@Model.Description</p>
<p>@Model.Last_Edit</p>
<p>@Model.Submitted</p>
<p>@Model.Impact</p>
<p>@Model.Effort</p>
<p>@Model.Priority</p>
<p>@Model.Status</p>
<p>@Model.Cost_Save</p>
<p>@Model.Challenges</p>
<p>@Model.Results</p>

@foreach (var item in Model.Hashtags)
{
    <div>
        <a href="Idea/@item.Id">@item.Tag</a>
        <hr />
    </div>
}
